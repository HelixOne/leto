{
  "name": "leto",
  "version": "0.0.2",
  "description": "Leto real-time appication.",
  "main": "dist/electron/main.bundle.js",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typings": "typings",
    "webpack": "webpack",
    "clean:dist": "npm run rimraf -- dist",
    "clean:www": "npm run rimraf -- dist/www",
    "clean:tmp": "npm run rimraf -- tmp",
    "clean:electron": "npm run rimraf -- dist/electron",
    "clean:server": "npm run rimraf -- dist/server",
    "build": "npm run build:prod && npm run build:server && npm run build:electron",
    "build:aot": "webpack --config webpack.config.aot.js",
    "build:electron": "npm run clean:electron && webpack --config ./config/webpack.electron.js --progress --profile --colors --display-error-details --display-cached",
    "build:server": "npm run clean:server && webpack --config ./config/webpack.server.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run aot",
    "horizon": "hz serve . --dev",
    "start": "npm run build",
    "electron": "electron ./dist/electron/main.bundle.js",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "gulp": "gulp",
    "aot": "npm run clean:tmp && npm run clean:www && npm run gulp && npm run ngc  && npm run build:aot && npm run clean:tmp"
  },
