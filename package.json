{
  "name": "leto",
  "version": "0.1.0",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "clean:dist": "npm run rimraf -- dist",
    "clean:www": "npm run rimraf -- dist/www",
    "clean:tmp": "npm run rimraf -- tmp",
    "clean:electron": "npm run rimraf -- dist/electron",
    "clean:server": "npm run rimraf -- dist/server",
    "build": "npm run clean:dist && npm run build:aot & npm run build:server & npm run build:electron & wait",
    "build:watch": "npm run build:server:watch & npm run build:www:watch",
    "build:www:watch": "webpack --config webpack.config.js --progress --profile --watch",
    "build:aot": "webpack --config webpack.config.aot.js",
    "build:sw": "webpack --config webpack.config.sw.js",
    "build:electron": "npm run clean:electron && webpack --config ./webpack.electron.js --progress --profile --colors --display-error-details --display-cached",
    "build:server": "npm run clean:server && webpack --config ./webpack.server.js --progress --profile --colors --display-error-details --display-cached",
    "build:server:watch": "npm run build:server -- --watch",
    "horizon": "hz serve . --dev",
    "start": "npm run dev",
    "electron:run": "electron ./dist/electron/electron.js",
    "aot": "npm run clean:www  && npm run build:aot",
    "webpack-dev-server": "webpack-dev-server",
    "server:dev": "webpack-dev-server --config webpack.config.js --progress --profile --watch --content-base src/",
    "dev": "npm run server:dev",
    "electron:osx": "./node_modules/electron-packager/cli.js dist/electron Leto --platform=darwin --icon=leto.icns --arch=x64 --out=dist/osx --overwrite",
    "electron:win": "./node_modules/electron-packager/cli.js dist/electron Leto --platform=win32 --arch=x64 --out=dist/osx --overwrite",
    "electron:pack": "./node_modules/electron-packager/cli.js dist/electron Leto --all"
  },
  "license": "MIT",
  "author": "Zubair Lawrence <zubair@helix.one>",
  "repository": {
    "type": "git",
    "url": "git"
  },
  "flat": true,
